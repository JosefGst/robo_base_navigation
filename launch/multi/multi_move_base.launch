<launch>    
  <!--- Run AMCL -->
  <include file="$(find robo_base_navigation)/launch/amcl.launch" />

  <!-- Define your move_base node -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find robo_base_navigation)/param/multi_move_base/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robo_base_navigation)/param/multi_move_base/costmap_common_params.yaml" command="load" ns="local_costmap" />

    <rosparam file="$(find robo_base_navigation)/param/multi_move_base/map_nav_params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find robo_base_navigation)/param/multi_move_base/map_nav_params/local_costmap_params.yaml" command="load" />

    <rosparam file="$(find robo_base_navigation)/param/multi_move_base/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find robo_base_navigation)/param/multi_move_base/move_base_params.yaml" command="load" />

    <param name="base_global_planner" type="string" value="navfn/NavfnROS" />
    <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>

    <remap from="map" to="/map" />
  </node>
</launch>